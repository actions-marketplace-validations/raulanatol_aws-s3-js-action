{
  "name": "aws-s3-js-action",
  "version": "0.1.0",
  "private": true,
  "description": "Synchronize S3 bucket files",
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "license": "MIT",
  "author": "raulanatol",
  "main": "lib/main.js",
  "scripts": {
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint --max-warnings 0 . --ext .ts,.tsx",
    "pack": "ncc build",
    "test": "jest"
  },
  "dependencies": {
    "@actions/core": "^1.2.5",
    "aws-sdk": "^2.740.0",
    "recursive-readdir": "^2.2.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@typescript-eslint/parser": "^3.10.1",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.7.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^23.20.0",
    "graceful-fs": "^4.2.4",
    "jest": "^26.4.2",
    "jest-circus": "^26.4.2",
    "js-yaml": "^3.14.0",
    "mime": ">=2.4.6",
    "prettier": "^2.1.1",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  }
}
